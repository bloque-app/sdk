{
  "name": "@bloque/sdk",
  "version": "0.0.2",
  "description": "Official Bloque SDK",
  "type": "module",
  "keywords": [
    "bloque",
    "sdk",
    "api"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "git+https://github.com/bloque-app/sdk.git#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloque-app/sdk.git",
    "directory": "packages/sdk"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./init": {
      "types": "./dist/init.d.ts",
      "import": "./dist/init.js",
      "require": "./dist/init.cjs"
    },
    "./accounts": {
      "types": "./dist/accounts.d.ts",
      "import": "./dist/accounts.js",
      "require": "./dist/accounts.cjs"
    },
    "./compliance": {
      "types": "./dist/compliance.d.ts",
      "import": "./dist/compliance.js",
      "require": "./dist/compliance.cjs"
    },
    "./orgs": {
      "types": "./dist/orgs.d.ts",
      "import": "./dist/orgs.js",
      "require": "./dist/orgs.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "clean": "rm -rf node_modules && rm -rf dist",
    "check": "biome check --write",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@bloque/sdk-accounts": "workspace:*",
    "@bloque/sdk-core": "workspace:*",
    "@bloque/sdk-compliance": "workspace:*",
    "@bloque/sdk-orgs": "workspace:*"
  },
  "devDependencies": {
    "@rslib/core": "catalog:",
    "@types/node": "catalog:",
    "@typescript/native-preview": "catalog:",
    "typescript": "catalog:"
  }
}
